<data>
    <record id="cron_fetch_biometric_logs" model="ir.cron">
        <field name="name">FLC Fetch Biometric Logs</field>
        <field name="model_id" ref="flc_biometric_integration.model_biometric_attendance_wizard"/>
        <field name="state">code</field>
        <field name="code">model.cron_fetch_attendance_logs()</field>
        <field name="interval_type">minutes</field>
        <field name="interval_number">60</field> <!-- Change to 1 for every minute -->
        <field name="active" eval="True"/>
    </record>
</data>
